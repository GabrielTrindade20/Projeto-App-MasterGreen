{
  "expo": {
    "name": "MasterGreen",
    "slug": "AppMasterGreen",
    "version": "1.9.8",
    "orientation": "portrait",
    "icon": "./assets/logoSemFundo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logoSemFundo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logoSemFundo.png",
        "backgroundColor": "#ffffff",
        "blockedPermissions": [
          "android.permission.RECORD_AUDIO"
        ]
      },
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "package": "com.gab_roch.AppMasterGreen"
    },
    "package": "com.gab_roch.MasterGreen",
    "extra": {
      "eas": {
        "projectId": "17ee6e08-809f-43e3-9d61-3cc78e06ea52"
      }
    }
  },
  "web": {
    "favicon": "./assets/favicon.png"
  },
  "extra": {
    "eas": {
      "projectId": "17ee6e08-809f-43e3-9d61-3cc78e06ea52"
    }
  },
  "plugins": [
    [
      "expo-media-library",
      {
        "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
        "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos."
      }
    ],
    [
      "expo-image-picker",
      {
        "photosPermission": "The app accesses your photos to let you share them with your friends."
      }
    ],
    [
      "expo-document-picker",
      {
        "iCloudContainerEnvironment": "Production"
      }
    ]
  ]
}
